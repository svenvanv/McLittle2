@model IEnumerable<McLittle.Models.SubCategory>

@{
    ViewBag.Title = "subcat";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>

                @Html.ActionLink("SubSubcategory", "SubSubcategory", new { id = item.SubCategoryId }) |
                @Html.ActionLink("Details", "Details", new { id = item.SubCategoryId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.SubCategoryId })
            </td>
        </tr>
    }

</table>
