@model IEnumerable<McLittle.Models.SubSubCategory>

@{
    ViewBag.Title = "SubSubcat";
}

<div class="Ts_header">
    <div class="Ts_dark_overlay Ts_categorieen_header">
        <div class="jumbotron">
            <h1>SubCategorie</h1>
            <p class="lead">Kies een product subcategorie!</p>
        </div>
    </div>
</div>
<div class="empty_space2"></div>
@foreach (var item in Model)
{
    <div class="col-lg-4 col-sm-6 Ts_Catgorie">
        <label>
            @Html.ActionLink(item.Title, "Products", new { id = item.SubSubCategoryId })
        </label>
    </div>

}
    <div class="empty_space2"></div>
<label class="Ts_vorige_pagina_btn">
    @Html.ActionLink("Terug Naar vorige pagina", null, null, null, new { href = Request.UrlReferrer })
</label>
<div class="empty_space2"></div>




